#include<bits/stdc++.h>
using namespace std;

string stringTransformation(string str){
    // for-loop use 

    for(int i=0; i< str.size(); i++){
        if(str[i]=='a')
        str[i]='b';
        else if  ( str[i]=='b')
        str[i]='a';

    }
    return str;
}
int main(){
    string str="abaabbcc";
    cout<<stringTransformation(str);
    return 0;
}
